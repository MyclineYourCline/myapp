<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ActivityManager.WareHouseActivity">
    <com.google.android.material.tabs.TabLayout
        android:id="@+id/WareHouseActivity_tab"
        app:tabSelectedTextColor="@color/white"
        app:tabIndicatorColor="#EF3232"
        app:tabBackground="@drawable/custom_tab"
        android:elevation="4dp"
        android:layout_toEndOf="@+id/WareHouseActivity_back"
        android:layout_width="match_parent"
        android:layout_height="?actionBarSize"/>
    <ImageView
        android:id="@+id/WareHouseActivity_back"
        android:background="#EF3232"
        android:stateListAnimator="@animator/btn_click_animator"
        android:src="@drawable/baseline_arrow_back_24"
        android:layout_width="50dp"
        android:padding="10dp"
        android:layout_height="?actionBarSize"/>
    <androidx.viewpager2.widget.ViewPager2
        android:id="@+id/WareHouseActivity_container"
        android:layout_below="@+id/WareHouseActivity_tab"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</RelativeLayout>